{
    "version": "0.0.1",
    "name": "larasdks/zoom",
    "description": "A Laravel package by laraSDKs to facilitate seamless integration with the Zoom API.",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^12.5|^11.44",
        "illuminate/http": "^12.5|^11.44",
        "laravel/socialite": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "laraSDKs\\Zoom\\": "src/"
        }
    },
    "authors": [
        {
            "role": "Developer",
            "name": "Hamza Alayed",
            "email": "developerh108@gmail.com",
            "homepage": "https://hamzax.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "laraSDKs\\Zoom\\Providers\\ZoomServiceProvider"
            ],
            "aliases": {
                "Zoom": "laraSDKs\\Zoom\\Facades\\Zoom"
            }
        }
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}

